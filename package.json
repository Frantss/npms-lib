{
  "name": "npms-lib",
  "version": "0.1.0",
  "description": "Typed API for npms",
  "keywords": [
    "typescript",
    "npms",
    "api"
  ],
  "repository": "https://github.com/Frantss/npms-lib",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./dist/index.esm.js",
    "require": "./dist/index.cjs.js",
    "umd": "./dist/index.umd.js"
  },
  "main": "dist/index.cjs.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.public.d.ts",
  "files": [
    "dist/index.public.d.ts",
    "dist/index.cjs.js",
    "dist/index.cjs.js.map",
    "dist/index.esm.js",
    "dist/index.esm.js.map",
    "dist/index.umd.js",
    "dist/index.umd.js.map"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c && api-extractor run --local",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@microsoft/api-extractor": "^7.7.13",
    "@release-it/conventional-changelog": "^2.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@types/jest": "^25.2.1",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "prettier": "^2.0.4",
    "release-it": "^14.2.1",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {},
  "npm": {
    "publish": true
  },
  "publishConfig": {
    "publish": true,
    "registry": "https://registry.npmjs.org/"
  },
  "authors": [
    "Frantss Bongiovanni <frantss.bongiovanni@gmail.com>"
  ],
  "contributors": [
    "Frantss Bongiovanni <frantss.bongiovanni@gmail.com>"
  ]
}
